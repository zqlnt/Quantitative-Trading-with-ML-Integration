# Neural Quant Environment Variables
# Copy this file to .env and update with your actual values

# Environment
NEURAL_QUANT_ENV=development

# MLflow Configuration
MLFLOW_TRACKING_URI=sqlite:///mlflow.db
MLFLOW_EXPERIMENT_NAME=neural_quant_experiments

# Data Source API Keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
POLYGON_API_KEY=your_polygon_api_key_here
IEX_CLOUD_API_KEY=your_iex_cloud_api_key_here

# Broker API Keys
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here

# Database Configuration (for production)
# DATABASE_HOST=localhost
# DATABASE_PORT=5432
# DATABASE_USERNAME=neural_quant
# DATABASE_PASSWORD=your_database_password
# DATABASE_NAME=neural_quant

# Notification Configuration
# EMAIL_SMTP_SERVER=smtp.gmail.com
# EMAIL_SMTP_PORT=587
# EMAIL_USERNAME=your_email@gmail.com
# EMAIL_PASSWORD=your_app_password
# SLACK_WEBHOOK_URL=your_slack_webhook_url
# DISCORD_WEBHOOK_URL=your_discord_webhook_url
